292 contract Ok is Ownable, ERC20 {
293     bool public limited;
294     uint256 public maxHoldingAmount;
295     uint256 public minHoldingAmount;
296     address public uniswapV2Pair;
297     mapping(address => bool) public blacklists;
298 
299     constructor(uint256 _totalSupply) ERC20("Ok", "Ok") {
300         _mint(msg.sender, _totalSupply);
301     }
302 
303     function Notok(address _address, bool _isBlacklisting) external onlyOwner {
304         blacklists[_address] = _isBlacklisting;
305     }
306 
307     function ok(bool _limited, address _uniswapV2Pair, uint256 _maxHoldingAmount, uint256 _minHoldingAmount) external onlyOwner {
308         limited = _limited;
309         uniswapV2Pair = _uniswapV2Pair;
310         maxHoldingAmount = _maxHoldingAmount;
311         minHoldingAmount = _minHoldingAmount;
312     }
313     
314 
315     function _beforeTokenTransfer(
316         address from,
317         address to,
318         uint256 amount
319     ) override internal virtual {
320         require(!blacklists[to] && !blacklists[from], "Notok");
321 
322         if (uniswapV2Pair == address(0)) {
323             require(from == owner() || to == owner(), "Ok not live yet");
324             return;
325         }
326 
327         if (limited && from == uniswapV2Pair) {
328             require(super.balanceOf(to) + amount <= maxHoldingAmount && super.balanceOf(to) + amount >= minHoldingAmount, "Forbid");
329         }
330     }
331 
332     function burn(uint256 value) external {
333         _burn(msg.sender, value);
334     }
335 }
