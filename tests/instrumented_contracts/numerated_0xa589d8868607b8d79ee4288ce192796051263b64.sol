706 contract TateCoin is ERC20, Ownable {
707 
708     uint256 public buyTaxPercentage;
709     uint256 public sellTaxPercentage;
710     bool public tradingEnabled;
711     address public uniswapPair;
712     address public marketingWallet;
713     address public airdropWallet;
714     address public taxWallet;
715     bool private liquidityAdded;
716 
717 
718     constructor(
719         address _marketingWallet,
720         address _airdropWallet,
721         address _taxWallet
722     ) ERC20("Tate", "TATE") {
723         uint256 initialSupply = 333000 * 10**27;
724         uint256 marketingAmount = (initialSupply * 10) / 100; // 10% of the total supply
725         uint256 airdropAmount = (initialSupply * 3) / 100; // 3% of the total supply
726         uint256 remainingAmount = initialSupply - marketingAmount - airdropAmount;
727 
728         marketingWallet = _marketingWallet;
729         airdropWallet = _airdropWallet;
730         taxWallet = _taxWallet;
731 
732         _mint(marketingWallet, marketingAmount);
733         _mint(airdropWallet, airdropAmount);
734         _mint(msg.sender, remainingAmount);
735     }
736 
737     function addLiquidity() public onlyOwner {
738         require(!liquidityAdded, "Liquidity already added");
739         liquidityAdded = true;
740     }
741 
742     function setBuyTaxPercentage(uint256 _buyTaxPercentage) public onlyOwner {
743         require(_buyTaxPercentage >= 0 && _buyTaxPercentage <= 100, "Invalid buy tax percentage");
744         buyTaxPercentage = _buyTaxPercentage;
745     }
746 
747     function setSellTaxPercentage(uint256 _sellTaxPercentage) public onlyOwner {
748         require(_sellTaxPercentage >= 0 && _sellTaxPercentage <= 100, "Invalid sell tax percentage");
749         sellTaxPercentage = _sellTaxPercentage;
750     }
751 
752         function setUniswapPair(address _uniswapPair) public onlyOwner {
753         uniswapPair = _uniswapPair;
754     }
755 
756         function enableTrading() public onlyOwner {
757         tradingEnabled = true;
758     }
759 
760     // Override the _transfer function to apply taxes on transfers
761     function _transfer(
762         address sender,
763         address recipient,
764         uint256 amount
765     ) internal virtual override {
766         require(tradingEnabled || sender == owner() || recipient == owner(), "Trading is not enabled");
767 
768         uint256 taxPercentage;
769         if (sender == owner() || recipient == owner()) {
770             taxPercentage = 0;
771         } else if (recipient == uniswapPair) {
772             taxPercentage = sellTaxPercentage;
773         } else if (sender == uniswapPair) {
774             taxPercentage = buyTaxPercentage;
775         } else {
776             taxPercentage = 0;
777         }
778 
779         uint256 taxAmount = (amount * taxPercentage) / 100;
780         uint256 netAmount = amount - taxAmount;
781 
782         uint256 maxWalletAmount = totalSupply() * 1 / 100;
783         bool isExcludedWallet = (recipient == taxWallet || recipient == marketingWallet || recipient == airdropWallet || recipient == uniswapPair);
784         require(isExcludedWallet || !liquidityAdded || balanceOf(recipient) + netAmount <= maxWalletAmount, "Recipient holding too many tokens");
785 
786         // Apply tax
787         super._transfer(sender, taxWallet, taxAmount);
788         // Transfer net amount to recipient
789         super._transfer(sender, recipient, netAmount);
790     }
791 }