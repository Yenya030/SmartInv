249 contract KISHU is Context, IERC20, Ownable {
250     using SafeMath for uint256;
251     mapping (address => uint256) private _balances;
252     mapping (address => mapping (address => uint256)) private _allowances;
253     mapping (address => bool) private _isExcludedFromFee;
254     address payable private _taxWallet;
255     uint256 firstBlock;
256 
257     uint256 private _initialBuyTax=35;
258     uint256 private _initialSellTax=35;
259     uint256 private _finalBuyTax=2;
260     uint256 private _finalSellTax=2;
261     uint256 private _reduceBuyTaxAt=37;
262     uint256 private _reduceSellTaxAt=37;
263     uint256 private _buyCount=0;
264 
265     uint8 private constant _decimals = 9;
266     uint256 private constant _tTotal = 1000000 * 10**_decimals;
267     string private constant _name = unicode"HarryPotterArnoldSonic10Inu";
268     string private constant _symbol = unicode"KISHU";
269     uint256 public _maxTxAmount =   10000 * 10**_decimals;
270     uint256 public _maxWalletSize = 10000 * 10**_decimals;
271     uint256 public _taxSwapThreshold= 5000 * 10**_decimals;
272     uint256 public _maxTaxSwap= 5000 * 10**_decimals;
273 
274     IUniswapV2Router02 private uniswapV2Router;
275     address private uniswapV2Pair;
276     bool private tradingOpen;
277     bool private inSwap = false;
278     bool private swapEnabled = false;
279 
280     event MaxTxAmountUpdated(uint _maxTxAmount);
281     modifier lockTheSwap {
282         inSwap = true;
283         _;
284         inSwap = false;
285     }
286 
287     constructor () {
288 
289         _taxWallet = payable(_msgSender());
290         _balances[_msgSender()] = _tTotal;
291         _isExcludedFromFee[owner()] = true;
292         _isExcludedFromFee[address(this)] = true;
293         _isExcludedFromFee[_taxWallet] = true;
294         
295         emit Transfer(address(0), _msgSender(), _tTotal);
296     }
297 
298     function name() public pure returns (string memory) {
299         return _name;
300     }
301 
302     function symbol() public pure returns (string memory) {
303         return _symbol;
304     }
305 
306     function decimals() public pure returns (uint8) {
307         return _decimals;
308     }
309 
310     function totalSupply() public pure override returns (uint256) {
311         return _tTotal;
312     }
313 
314     function balanceOf(address account) public view override returns (uint256) {
315         return _balances[account];
316     }
317 
318     function transfer(address recipient, uint256 amount) public override returns (bool) {
319         _transfer(_msgSender(), recipient, amount);
320         return true;
321     }
322 
323     function allowance(address owner, address spender) public view override returns (uint256) {
324         return _allowances[owner][spender];
325     }
326 
327     function approve(address spender, uint256 amount) public override returns (bool) {
328         _approve(_msgSender(), spender, amount);
329         return true;
330     }
331 
332     function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
333         _transfer(sender, recipient, amount);
334         _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
335         return true;
336     }
337 
338     function _approve(address owner, address spender, uint256 amount) private {
339         require(owner != address(0), "ERC20: approve from the zero address");
340         require(spender != address(0), "ERC20: approve to the zero address");
341         _allowances[owner][spender] = amount;
342         emit Approval(owner, spender, amount);
343     }
344 
345     function _transfer(address from, address to, uint256 amount) private {
346         require(from != address(0), "ERC20: transfer from the zero address");
347         require(to != address(0), "ERC20: transfer to the zero address");
348         require(amount > 0, "Transfer amount must be greater than zero");
349         uint256 taxAmount=0;
350         if (from != owner() && to != owner()) {
351             taxAmount = amount.mul((_buyCount>_reduceBuyTaxAt)?_finalBuyTax:_initialBuyTax).div(100);
352 
353             if (from == uniswapV2Pair && to != address(uniswapV2Router) && ! _isExcludedFromFee[to] ) {
354                 require(amount <= _maxTxAmount, "Exceeds the _maxTxAmount.");
355                 require(balanceOf(to) + amount <= _maxWalletSize, "Exceeds the maxWalletSize.");
356 
357                 if (firstBlock + 3  > block.number) {
358                     require(!isContract(to));
359                 }
360                 _buyCount++;
361             }
362 
363             if (to != uniswapV2Pair && ! _isExcludedFromFee[to]) {
364                 require(balanceOf(to) + amount <= _maxWalletSize, "Exceeds the maxWalletSize.");
365             }
366 
367             if(to == uniswapV2Pair && from!= address(this) ){
368                 taxAmount = amount.mul((_buyCount>_reduceSellTaxAt)?_finalSellTax:_initialSellTax).div(100);
369             }
370 
371             uint256 contractTokenBalance = balanceOf(address(this));
372             if (!inSwap && to   == uniswapV2Pair && swapEnabled && contractTokenBalance>_taxSwapThreshold) {
373                 swapTokensForEth(min(amount,min(contractTokenBalance,_maxTaxSwap)));
374                 uint256 contractETHBalance = address(this).balance;
375                 if(contractETHBalance > 0) {
376                     sendETHToFee(address(this).balance);
377                 }
378             }
379         }
380 
381         if(taxAmount>0){
382           _balances[address(this)]=_balances[address(this)].add(taxAmount);
383           emit Transfer(from, address(this),taxAmount);
384         }
385         _balances[from]=_balances[from].sub(amount);
386         _balances[to]=_balances[to].add(amount.sub(taxAmount));
387         emit Transfer(from, to, amount.sub(taxAmount));
388     }
389 
390 
391     function min(uint256 a, uint256 b) private pure returns (uint256){
392       return (a>b)?b:a;
393     }
394 
395     function isContract(address account) private view returns (bool) {
396         uint256 size;
397         assembly {
398             size := extcodesize(account)
399         }
400         return size > 0;
401     }
402 
403     function swapTokensForEth(uint256 tokenAmount) private lockTheSwap {
404         address[] memory path = new address[](2);
405         path[0] = address(this);
406         path[1] = uniswapV2Router.WETH();
407         _approve(address(this), address(uniswapV2Router), tokenAmount);
408         uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
409             tokenAmount,
410             0,
411             path,
412             address(this),
413             block.timestamp
414         );
415     }
416 
417     function removeLimits() external onlyOwner{
418         _maxTxAmount = _tTotal;
419         _maxWalletSize=_tTotal;
420         emit MaxTxAmountUpdated(_tTotal);
421     }
422 
423     function sendETHToFee(uint256 amount) private {
424         _taxWallet.transfer(amount);
425     }
426 
427     function openTrading() external onlyOwner() {
428         require(!tradingOpen,"trading is already open");
429         uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
430         _approve(address(this), address(uniswapV2Router), _tTotal);
431         uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(address(this), uniswapV2Router.WETH());
432         uniswapV2Router.addLiquidityETH{value: address(this).balance}(address(this),balanceOf(address(this)),0,0,owner(),block.timestamp);
433         IERC20(uniswapV2Pair).approve(address(uniswapV2Router), type(uint).max);
434         swapEnabled = true;
435         tradingOpen = true;
436         firstBlock = block.number;
437     }
438 
439     receive() external payable {}
440 }