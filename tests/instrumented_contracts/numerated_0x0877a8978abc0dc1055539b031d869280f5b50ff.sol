1535     function swapBack() private {
1536         uint256 contractBalance = balanceOf(address(this));
1537         uint256 totalTokensToSwap = tokensForLiquidity +
1538             tokensForMarketing +
1539             tokensForDev;
1540         bool success;
1541 
1542         if (contractBalance == 0 || totalTokensToSwap == 0) {
1543             return;
1544         }
1545 
1546         if (contractBalance > swapTokensAtAmount * 20) {
1547             contractBalance = swapTokensAtAmount * 20;
1548         }
1549 
1550         // Halve the amount of liquidity tokens
1551         uint256 liquidityTokens = (contractBalance * tokensForLiquidity) /
1552             totalTokensToSwap /
1553             2;
1554         uint256 amountToSwapForETH = contractBalance.sub(liquidityTokens);
1555 
1556         uint256 initialETHBalance = address(this).balance;
1557 
1558         swapTokensForEth(amountToSwapForETH);
1559 
1560         uint256 ethBalance = address(this).balance.sub(initialETHBalance);
1561 
1562         uint256 ethForMarketing = ethBalance.mul(tokensForMarketing).div(
1563             totalTokensToSwap
1564         );
1565         uint256 ethForDev = ethBalance.mul(tokensForDev).div(totalTokensToSwap);
1566 
1567         uint256 ethForLiquidity = ethBalance - ethForMarketing - ethForDev;
1568 
1569         tokensForLiquidity = 0;
1570         tokensForMarketing = 0;
1571         tokensForDev = 0;
1572 
1573         (success, ) = address(devWallet).call{value: ethForDev}("");
1574 
1575         if (liquidityTokens > 0 && ethForLiquidity > 0) {
1576             addLiquidity(liquidityTokens, ethForLiquidity);
1577             emit SwapAndLiquify(
1578                 amountToSwapForETH,
1579                 ethForLiquidity,
1580                 tokensForLiquidity
1581             );
1582         }
1583 
1584         (success, ) = address(marketingWallet).call{
1585             value: address(this).balance
1586         }("");
1587     }
