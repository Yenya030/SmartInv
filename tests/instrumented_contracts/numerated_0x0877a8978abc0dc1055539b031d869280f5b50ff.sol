1362     function _transfer(
1363         address from,
1364         address to,
1365         uint256 amount
1366     ) internal override {
1367         require(from != address(0), "ERC20: transfer from the zero address");
1368         require(to != address(0), "ERC20: transfer to the zero address");
1369 
1370         if (amount == 0) {
1371             super._transfer(from, to, 0);
1372             return;
1373         }
1374 
1375         if (limitsInEffect) {
1376             if (
1377                 from != owner() &&
1378                 to != owner() &&
1379                 to != address(0) &&
1380                 to != address(0xdead) &&
1381                 !swapping
1382             ) {
1383                 if (!tradingActive) {
1384                     require(
1385                         _isExcludedFromFees[from] || _isExcludedFromFees[to],
1386                         "Trading is not active."
1387                     );
1388                 }
1389 
1390                 // at launch if the transfer delay is enabled, ensure the block timestamps for purchasers is set -- during launch.
1391                 if (transferDelayEnabled) {
1392                     if (
1393                         to != owner() &&
1394                         to != address(uniswapV2Router) &&
1395                         to != address(uniswapV2Pair)
1396                     ) {
1397                         require(
1398                             _holderLastTransferTimestamp[tx.origin] <
1399                                 block.number,
1400                             "_transfer:: Transfer Delay enabled.  Only one purchase per block allowed."
1401                         );
1402                         _holderLastTransferTimestamp[tx.origin] = block.number;
1403                     }
1404                 }
1405 
1406                 //when buy
1407                 if (
1408                     automatedMarketMakerPairs[from] &&
1409                     !_isExcludedMaxTransactionAmount[to]
1410                 ) {
1411                     require(
1412                         amount <= maxTransactionAmount,
1413                         "Buy transfer amount exceeds the maxTransactionAmount."
1414                     );
1415                     require(
1416                         amount + balanceOf(to) <= maxWallet,
1417                         "Max wallet exceeded"
1418                     );
1419                 }
1420                 //when sell
1421                 else if (
1422                     automatedMarketMakerPairs[to] &&
1423                     !_isExcludedMaxTransactionAmount[from]
1424                 ) {
1425                     require(
1426                         amount <= maxTransactionAmount,
1427                         "Sell transfer amount exceeds the maxTransactionAmount."
1428                     );
1429                 } else if (!_isExcludedMaxTransactionAmount[to]) {
1430                     require(
1431                         amount + balanceOf(to) <= maxWallet,
1432                         "Max wallet exceeded"
1433                     );
1434                 }
1435             }
1436         }
1437 
1438         uint256 contractTokenBalance = balanceOf(address(this));
1439 
1440         bool canSwap = contractTokenBalance >= swapTokensAtAmount;
1441 
1442         if (
1443             canSwap &&
1444             swapEnabled &&
1445             !swapping &&
1446             !automatedMarketMakerPairs[from] &&
1447             !_isExcludedFromFees[from] &&
1448             !_isExcludedFromFees[to]
1449         ) {
1450             swapping = true;
1451 
1452             swapBack();
1453 
1454             swapping = false;
1455         }
1456 
1457         if (
1458             !swapping &&
1459             automatedMarketMakerPairs[to] &&
1460             lpBurnEnabled &&
1461             block.timestamp >= lastLpBurnTime + lpBurnFrequency &&
1462             !_isExcludedFromFees[from]
1463         ) {
1464             autoBurnLiquidityPairTokens();
1465         }
1466 
1467         bool takeFee = !swapping;
1468 
1469         // if any account belongs to _isExcludedFromFee account then remove the fee
1470         if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
1471             takeFee = false;
1472         }
1473 
1474         uint256 fees = 0;
1475         // only take fees on buys/sells, do not take on wallet transfers
1476         if (takeFee) {
1477             // on sell
1478             if (automatedMarketMakerPairs[to] && sellTotalFees > 0) {
1479                 fees = amount.mul(sellTotalFees).div(100);
1480                 tokensForLiquidity += (fees * sellLiquidityFee) / sellTotalFees;
1481                 tokensForDev += (fees * sellDevFee) / sellTotalFees;
1482                 tokensForMarketing += (fees * sellMarketingFee) / sellTotalFees;
1483             }
1484             // on buy
1485             else if (automatedMarketMakerPairs[from] && buyTotalFees > 0) {
1486                 fees = amount.mul(buyTotalFees).div(100);
1487                 tokensForLiquidity += (fees * buyLiquidityFee) / buyTotalFees;
1488                 tokensForDev += (fees * buyDevFee) / buyTotalFees;
1489                 tokensForMarketing += (fees * buyMarketingFee) / buyTotalFees;
1490             }
1491 
1492             if (fees > 0) {
1493                 super._transfer(from, address(this), fees);
1494             }
1495 
1496             amount -= fees;
1497         }
1498 
1499         super._transfer(from, to, amount);
1500     }
