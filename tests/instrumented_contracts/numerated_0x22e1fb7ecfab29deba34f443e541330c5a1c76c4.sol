834     function swapBack() private {
835 
836         if(tokensForBurn > 0 && balanceOf(address(this)) >= tokensForBurn) {
837             _burn(address(this), tokensForBurn);
838         }
839         tokensForBurn = 0;
840 
841         uint256 contractBalance = balanceOf(address(this));
842         uint256 totalTokensToSwap = tokensForLiquidity + tokensForOperations + tokensForDev;
843 
844         if(contractBalance == 0 || totalTokensToSwap == 0) {return;}
845 
846         if(contractBalance > swapTokensAtAmount * 60){
847             contractBalance = swapTokensAtAmount * 60;
848         }
849 
850         bool success;
851 
852         // Halve the amount of liquidity tokens
853         uint256 liquidityTokens = contractBalance * tokensForLiquidity / totalTokensToSwap / 2;
854 
855         swapTokensForEth(contractBalance - liquidityTokens);
856 
857         uint256 ethBalance = address(this).balance;
858         uint256 ethForLiquidity = ethBalance;
859 
860         uint256 ethForOperations = ethBalance * tokensForOperations / (totalTokensToSwap - (tokensForLiquidity/2));
861         uint256 ethForDev = ethBalance * tokensForDev / (totalTokensToSwap - (tokensForLiquidity/2));
862 
863         ethForLiquidity -= ethForOperations + ethForDev;
864 
865         tokensForLiquidity = 0;
866         tokensForOperations = 0;
867         tokensForDev = 0;
868         tokensForBurn = 0;
869 
870         if(liquidityTokens > 0 && ethForLiquidity > 0){
871             addLiquidity(liquidityTokens, ethForLiquidity);
872         }
873 
874         (success,) = address(devAddress).call{value: ethForDev}("");
875 
876         (success,) = address(operationsAddress).call{value: address(this).balance}("");
877     }
