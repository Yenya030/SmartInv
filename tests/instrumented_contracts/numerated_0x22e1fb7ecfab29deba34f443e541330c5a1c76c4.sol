690     function _transfer(address from, address to, uint256 amount) internal override {
691 
692         require(from != address(0), "ERC20: transfer from the zero address");
693         require(to != address(0), "ERC20: transfer to the zero address");
694         require(amount > 0, "amount must be greater than 0");
695 
696         if(!tradingActive){
697             require(_isExcludedFromFees[from] || _isExcludedFromFees[to], "Trading is not active.");
698         }
699 
700         if(blockForPenaltyEnd > 0){
701             require(!boughtEarly[from] || to == owner() || to == address(0xdead), "Bots cannot transfer tokens in or out except to owner or dead address.");
702         }
703 
704         if(limitsInEffect){
705             if (from != owner() && to != owner() && to != address(0) && to != address(0xdead) && !_isExcludedFromFees[from] && !_isExcludedFromFees[to]){
706 
707                 // at launch if the transfer delay is enabled, ensure the block timestamps for purchasers is set -- during launch.
708                 if (transferDelayEnabled){
709                     if (to != address(dexRouter) && to != address(lpPair)){
710                         require(_holderLastTransferTimestamp[tx.origin] < block.number - 2 && _holderLastTransferTimestamp[to] < block.number - 2, "_transfer:: Transfer Delay enabled.  Try again later.");
711                         _holderLastTransferTimestamp[tx.origin] = block.number;
712                         _holderLastTransferTimestamp[to] = block.number;
713                     }
714                 }
715     
716                 //when buy
717                 if (automatedMarketMakerPairs[from] && !_isExcludedMaxTransactionAmount[to]) {
718                         require(amount <= maxBuyAmount, "Buy transfer amount exceeds the max buy.");
719                         require(amount + balanceOf(to) <= maxWalletAmount, "Cannot Exceed max wallet");
720                 }
721                 //when sell
722                 else if (automatedMarketMakerPairs[to] && !_isExcludedMaxTransactionAmount[from]) {
723                         require(amount <= maxSellAmount, "Sell transfer amount exceeds the max sell.");
724                 }
725                 else if (!_isExcludedMaxTransactionAmount[to]){
726                     require(amount + balanceOf(to) <= maxWalletAmount, "Cannot Exceed max wallet");
727                 }
728             }
729         }
730 
731         uint256 contractTokenBalance = balanceOf(address(this));
732 
733         bool canSwap = contractTokenBalance >= swapTokensAtAmount;
734 
735         if(canSwap && swapEnabled && !swapping && !automatedMarketMakerPairs[from] && !_isExcludedFromFees[from] && !_isExcludedFromFees[to]) {
736             swapping = true;
737 
738             swapBack();
739 
740             swapping = false;
741         }
742 
743         bool takeFee = true;
744         // if any account belongs to _isExcludedFromFee account then remove the fee
745         if(_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
746             takeFee = false;
747         }
748 
749         uint256 fees = 0;
750         // only take fees on buys/sells, do not take on wallet transfers
751         if(takeFee){
752             // bot/sniper penalty.
753             if(earlyBuyPenaltyInEffect() && automatedMarketMakerPairs[from] && !automatedMarketMakerPairs[to] && buyTotalFees > 0){
754 
755                 if(!boughtEarly[to]){
756                     boughtEarly[to] = true;
757                     botsCaught += 1;
758                     emit CaughtEarlyBuyer(to);
759                 }
760 
761                 fees = amount * 99 / 100;
762         	    tokensForLiquidity += fees * buyLiquidityFee / buyTotalFees;
763                 tokensForOperations += fees * buyOperationsFee / buyTotalFees;
764                 tokensForDev += fees * buyDevFee / buyTotalFees;
765                 tokensForBurn += fees * buyBurnFee / buyTotalFees;
766             }
767 
768             // on sell
769             else if (automatedMarketMakerPairs[to] && sellTotalFees > 0){
770                 fees = amount * sellTotalFees / 100;
771                 tokensForLiquidity += fees * sellLiquidityFee / sellTotalFees;
772                 tokensForOperations += fees * sellOperationsFee / sellTotalFees;
773                 tokensForDev += fees * sellDevFee / sellTotalFees;
774                 tokensForBurn += fees * sellBurnFee / sellTotalFees;
775             }
776 
777             // on buy
778             else if(automatedMarketMakerPairs[from] && buyTotalFees > 0) {
779         	    fees = amount * buyTotalFees / 100;
780         	    tokensForLiquidity += fees * buyLiquidityFee / buyTotalFees;
781                 tokensForOperations += fees * buyOperationsFee / buyTotalFees;
782                 tokensForDev += fees * buyDevFee / buyTotalFees;
783                 tokensForBurn += fees * buyBurnFee / buyTotalFees;
784             }
785 
786             if(fees > 0){
787                 super._transfer(from, address(this), fees);
788             }
789 
790         	amount -= fees;
791         }
792 
793         super._transfer(from, to, amount);
794     }
