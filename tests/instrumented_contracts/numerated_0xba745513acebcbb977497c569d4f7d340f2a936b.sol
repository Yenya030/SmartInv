247 contract InflationToken is ERC20Detailed, Owned, Pausable {
248     
249   using SafeMath for uint256;
250   mapping (address => uint256) private _balances;
251   mapping (address => mapping (address => uint256)) private _allowed;
252   
253   string constant tokenName = "Mainstream For The Underground";
254   string constant tokenSymbol = "MFTU";
255   mapping (address => bool) public _freezed;
256   uint8  constant tokenDecimals = 18;
