143 library SafeMath {
144   function mul(uint256 a, uint256 b) internal pure returns (uint256) {
145     if (a == 0) {
146       return 0;
147     }
148     uint256 c = a * b;
149     assert(c / a == b);
150     return c;
151   }
152   function div(uint256 a, uint256 b) internal pure returns (uint256) {
153     uint256 c = a / b;
154     return c;
155   }
156   function sub(uint256 a, uint256 b) internal pure returns (uint256) {
157     assert(b <= a);
158     return a - b;
159   }
160   function add(uint256 a, uint256 b) internal pure returns (uint256) {
161     uint256 c = a + b;
162     assert(c >= a);
163     return c;
164   }
165   function ceil(uint256 a, uint256 m) internal pure returns (uint256) {
166     uint256 c = add(a,m);
167     uint256 d = sub(c,1);
168     return mul(div(d,m),m);
169   }
170 }
