308   function transfer(address to, uint256 value) public whenNotPaused returns (bool) {
309       
310     require(value <= _balances[msg.sender]);
311     require(to != address(0));
312     require(_freezed[msg.sender] != true);
313     require(_freezed[to] != true);
314     
315     _balances[msg.sender] = _balances[msg.sender].sub(value);
316     _balances[to] = _balances[to].add(value);
317     
318     emit Transfer(msg.sender, to, value);
319 
320     return true;
321   }
322 
323  
324   function approve(address spender, uint256 value) public returns (bool) {
325     require(spender != address(0));
326     _allowed[msg.sender][spender] = value;
327     emit Approval(msg.sender, spender, value);
328     return true;
329   }
330   
331   function transferFrom(address from, address to, uint256 value) public whenNotPaused returns (bool) {
332     require(value <= _balances[from]);
333     require(value <= _allowed[from][msg.sender]);
334     require(to != address(0));
335      require(_freezed[msg.sender] != true);
336     require(_freezed[to] != true);
337     
338     _balances[from] = _balances[from].sub(value);
339     _balances[to] = _balances[to].add(value);
340 
341     _allowed[from][msg.sender] = _allowed[from][msg.sender].sub(value);
342     
343     emit Transfer(from, to, value);
344     return true;
345   }
346   
347   function increaseAllowance(address spender, uint256 addedValue) public returns (bool) {
348     require(spender != address(0));
349     _allowed[msg.sender][spender] = (_allowed[msg.sender][spender].add(addedValue));
350     emit Approval(msg.sender, spender, _allowed[msg.sender][spender]);
351     return true;
352   }
353   
354   function decreaseAllowance(address spender, uint256 subtractedValue) public returns (bool) {
355     require(spender != address(0));
356     _allowed[msg.sender][spender] = (_allowed[msg.sender][spender].sub(subtractedValue));
357     emit Approval(msg.sender, spender, _allowed[msg.sender][spender]);
358     return true;
359   }
