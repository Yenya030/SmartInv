214 contract Pausable is Owned {
215   event Pause();
216   event Unpause();
217   event NotPausable();
218 
219   bool public paused = false;
220   bool public canPause = true;
221 
222   modifier whenNotPaused() {
223     require(!paused || msg.sender == owner);
224     _;
225   }
226 
227   modifier whenPaused() {
228     require(paused);
229     _;
230   }
231 
232     function pause() onlyOwner whenNotPaused public {
233         require(canPause == true);
234         paused = true;
235         emit Pause();
236     }
237 
238   function unpause() onlyOwner whenPaused public {
239     require(paused == true);
240     paused = false;
241     emit Unpause();
242   }
243 }
