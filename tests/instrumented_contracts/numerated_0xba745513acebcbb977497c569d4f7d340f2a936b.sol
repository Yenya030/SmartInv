361   function _mint(address account, uint256 amount) onlyDeflationContractOrCurrent public returns(bool){
362     require(amount != 0);
363     _balances[account] = _balances[account].add(amount);
364     _totalSupply = _totalSupply.add(amount);
365 
366     emit Transfer(address(0), account, amount);
367     return true;
368   }
369   
370   function burn(uint256 amount) external onlyOwner {
371     _burn(msg.sender, amount);
372   }
373  
374   
375   function _burn(address account, uint256 amount) internal onlyOwner {
376     require(amount != 0);
377     require(amount <= _balances[account]);
378     _totalSupply = _totalSupply.sub(amount);
379     _balances[account] = _balances[account].sub(amount);
380     emit Transfer(account, address(0), amount);
381   }
382   
383   function burnFrom(address account, uint256 amount) external  {
384     require(amount <= _allowed[account][msg.sender]);
385     _allowed[account][msg.sender] = _allowed[account][msg.sender].sub(amount);
386     _burn(account, amount);
387   }
