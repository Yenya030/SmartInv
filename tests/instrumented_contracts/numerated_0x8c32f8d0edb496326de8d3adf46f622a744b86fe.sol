75 contract CAStoreContract is Ownable {
76   using SafeMath for uint;
77 
78   event DataStored(bytes32 field1, bytes32 field2, bytes32 field3, bytes32 field4);
79 
80   function storeData
81     (bytes32[] fields1, bytes32[] fields2, bytes32[] fields3, bytes32[] fields4) 
82     public onlyOwner {
83       for (uint i = 0; i < fields1.length; i++) {
84         emit DataStored(fields1[i], fields2[i], fields3[i], fields4[i]);
85       }
86     }
