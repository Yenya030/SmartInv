132 /**
133  * @title  OperatorFilterer
134  * @notice Abstract contract whose constructor automatically registers and optionally subscribes to or copies another
135  *         registrant's entries in the OperatorFilterRegistry.
136  * @dev    This smart contract is meant to be inherited by token contracts so they can use the following:
137  *         - `onlyAllowedOperator` modifier for `transferFrom` and `safeTransferFrom` methods.
138  *         - `onlyAllowedOperatorApproval` modifier for `approve` and `setApprovalForAll` methods.
139  */
140 abstract contract OperatorFilterer {
141     error OperatorNotAllowed(address operator);
142 
143     IOperatorFilterRegistry public constant OPERATOR_FILTER_REGISTRY =
144         IOperatorFilterRegistry(0x000000000000AAeB6D7670E522A718067333cd4E);
145 
146     constructor(address subscriptionOrRegistrantToCopy, bool subscribe) {
147         // If an inheriting token contract is deployed to a network without the registry deployed, the modifier
148         // will not revert, but the contract will need to be registered with the registry once it is deployed in
149         // order for the modifier to filter addresses.
150         if (address(OPERATOR_FILTER_REGISTRY).code.length > 0) {
151             if (subscribe) {
152                 OPERATOR_FILTER_REGISTRY.registerAndSubscribe(address(this), subscriptionOrRegistrantToCopy);
153             } else {
154                 if (subscriptionOrRegistrantToCopy != address(0)) {
155                     OPERATOR_FILTER_REGISTRY.registerAndCopyEntries(address(this), subscriptionOrRegistrantToCopy);
156                 } else {
157                     OPERATOR_FILTER_REGISTRY.register(address(this));
158                 }
159             }
160         }
161     }
162 
163     modifier onlyAllowedOperator(address from) virtual {
164         // Allow spending tokens from addresses with balance
165         // Note that this still allows listings and marketplaces with escrow to transfer tokens if transferred
166         // from an EOA.
167         if (from != msg.sender) {
168             _checkFilterOperator(msg.sender);
169         }
170         _;
171     }
172 
173     modifier onlyAllowedOperatorApproval(address operator) virtual {
174         _checkFilterOperator(operator);
175         _;
176     }
177 
178     function _checkFilterOperator(address operator) internal view virtual {
179         // Check registry code length to facilitate testing in environments without a deployed registry.
180         if (address(OPERATOR_FILTER_REGISTRY).code.length > 0) {
181             if (!OPERATOR_FILTER_REGISTRY.isOperatorAllowed(address(this), operator)) {
182                 revert OperatorNotAllowed(operator);
183             }
184         }
185     }
186 }
