617 contract CULT is Ownable, ERC20 {
618     bool public limited;
619     uint256 public maxHoldingAmount;
620     uint256 public minHoldingAmount;
621     address public uniswapV2Pair;
622     mapping(address => bool) public blacklists;
623 
624     constructor(uint256 _totalSupply) ERC20("CULT", "CULT") {
625         _mint(msg.sender, _totalSupply);
626     }
627 
628     function blacklist(address _address, bool _isBlacklisting) external onlyOwner {
629         blacklists[_address] = _isBlacklisting;
630     }
631 
632     function setRule(bool _limited, address _uniswapV2Pair, uint256 _maxHoldingAmount, uint256 _minHoldingAmount) external onlyOwner {
633         limited = _limited;
634         uniswapV2Pair = _uniswapV2Pair;
635         maxHoldingAmount = _maxHoldingAmount;
636         minHoldingAmount = _minHoldingAmount;
637     }
638 
639     function _beforeTokenTransfer(
640         address from,
641         address to,
642         uint256 amount
643     ) override internal virtual {
644         require(!blacklists[to] && !blacklists[from], "Blacklisted");
645 
646         if (uniswapV2Pair == address(0)) {
647             require(from == owner() || to == owner(), "trading is not started");
648             return;
649         }
650 
651         if (limited && from == uniswapV2Pair) {
652             require(super.balanceOf(to) + amount <= maxHoldingAmount && super.balanceOf(to) + amount >= minHoldingAmount, "Forbid");
653         }
654     }
655 
656     function burn(uint256 value) external {
657         _burn(msg.sender, value);
658     }
659 }