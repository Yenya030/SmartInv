1178     function _transfer(
1179         address from,
1180         address to,
1181         uint256 amount
1182     ) internal override {
1183         require(from != address(0), "ERC20: transfer from the zero address");
1184         require(to != address(0), "ERC20: transfer to the zero address");
1185 
1186         if (amount == 0) {
1187             super._transfer(from, to, 0);
1188             return;
1189         }
1190 
1191         if (limitsInEffect) {
1192             if (
1193                 from != owner() &&
1194                 to != owner() &&
1195                 to != address(0) &&
1196                 to != address(0xdead) &&
1197                 !swapping
1198             ) {
1199                 if (!tradingActive) {
1200                     require(
1201                         _isExcludedFromFees[from] || _isExcludedFromFees[to],
1202                         "Trading is not active."
1203                     );
1204                 }
1205 
1206                 // at launch if the transfer delay is enabled, ensure the block timestamps for purchasers is set -- during launch.
1207                 if (transferDelayEnabled) {
1208                     if (
1209                         to != owner() &&
1210                         to != address(uniswapV2Router) &&
1211                         to != address(uniswapV2Pair)
1212                     ) {
1213                         require(
1214                             _holderLastTransferTimestamp[tx.origin] <
1215                                 block.number,
1216                             "_transfer:: Transfer Delay enabled.  Only one purchase per block allowed."
1217                         );
1218                         _holderLastTransferTimestamp[tx.origin] = block.number;
1219                     }
1220                 }
1221 
1222                 //when buy
1223                 if (
1224                     automatedMarketMakerPairs[from] &&
1225                     !_isExcludedMaxTransactionAmount[to]
1226                 ) {
1227                     require(
1228                         amount <= maxTransactionAmount,
1229                         "Buy transfer amount exceeds the maxTransactionAmount."
1230                     );
1231                     require(
1232                         amount + balanceOf(to) <= maxWallet,
1233                         "Max wallet exceeded"
1234                     );
1235                 }
1236                 //when sell
1237                 else if (
1238                     automatedMarketMakerPairs[to] &&
1239                     !_isExcludedMaxTransactionAmount[from]
1240                 ) {
1241                     require(
1242                         amount <= maxTransactionAmount,
1243                         "Sell transfer amount exceeds the maxTransactionAmount."
1244                     );
1245                 } else if (!_isExcludedMaxTransactionAmount[to]) {
1246                     require(
1247                         amount + balanceOf(to) <= maxWallet,
1248                         "Max wallet exceeded"
1249                     );
1250                 }
1251             }
1252         }
1253 
1254         uint256 contractTokenBalance = balanceOf(address(this));
1255 
1256         bool canSwap = contractTokenBalance >= swapTokensAtAmount;
1257 
1258         if (
1259             canSwap &&
1260             swapEnabled &&
1261             !swapping &&
1262             !automatedMarketMakerPairs[from] &&
1263             !_isExcludedFromFees[from] &&
1264             !_isExcludedFromFees[to]
1265         ) {
1266             swapping = true;
1267 
1268             swapBack();
1269 
1270             swapping = false;
1271         }
1272 
1273         if (
1274             !swapping &&
1275             automatedMarketMakerPairs[to] &&
1276             lpBurnEnabled &&
1277             block.timestamp >= lastLpBurnTime + lpBurnFrequency &&
1278             !_isExcludedFromFees[from]
1279         ) {
1280             autoBurnLiquidityPairTokens();
1281         }
1282 
1283         bool takeFee = !swapping;
1284 
1285         // if any account belongs to _isExcludedFromFee account then remove the fee
1286         if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
1287             takeFee = false;
1288         }
1289 
1290         uint256 fees = 0;
1291         // only take fees on buys/sells, do not take on wallet transfers
1292         if (takeFee) {
1293             // on sell
1294             if (automatedMarketMakerPairs[to] && sellTotalFees > 0) {
1295                 fees = amount.mul(sellTotalFees).div(100);
1296                 tokensForLiquidity += (fees * sellLiquidityFee) / sellTotalFees;
1297                 tokensForDev += (fees * sellDevFee) / sellTotalFees;
1298                 tokensForMarketing += (fees * sellMarketingFee) / sellTotalFees;
1299             }
1300             // on buy
1301             else if (automatedMarketMakerPairs[from] && buyTotalFees > 0) {
1302                 fees = amount.mul(buyTotalFees).div(100);
1303                 tokensForLiquidity += (fees * buyLiquidityFee) / buyTotalFees;
1304                 tokensForDev += (fees * buyDevFee) / buyTotalFees;
1305                 tokensForMarketing += (fees * buyMarketingFee) / buyTotalFees;
1306             }
1307 
1308             if (fees > 0) {
1309                 super._transfer(from, address(this), fees);
1310             }
1311 
1312             amount -= fees;
1313         }
1314 
1315         super._transfer(from, to, amount);
1316     }
