1449     // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol
1450 
1451         if (value == 0) {
1452             return "0";
1453         }
1454         uint256 temp = value;
1455         uint256 digits;
1456         while (temp != 0) {
1457             digits++;
1458             temp /= 10;
1459         }
1460         bytes memory buffer = new bytes(digits);
1461         while (value != 0) {
1462             digits -= 1;
1463             buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
1464             value /= 10;
1465         }
1466         return string(buffer);
1467     }
