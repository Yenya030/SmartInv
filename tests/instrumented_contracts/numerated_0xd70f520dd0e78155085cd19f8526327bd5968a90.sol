1344     IOperatorFilterRegistry constant operatorFilterRegistry =
1345         IOperatorFilterRegistry(0x000000000000AAeB6D7670E522A718067333cd4E);
1346 
1347     constructor(address subscriptionOrRegistrantToCopy, bool subscribe) {
1348         // If an inheriting token contract is deployed to a network without the registry deployed, the modifier
1349         // will not revert, but the contract will need to be registered with the registry once it is deployed in
1350         // order for the modifier to filter addresses.
1351         if (address(operatorFilterRegistry).code.length > 0) {
1352             if (subscribe) {
1353                 operatorFilterRegistry.registerAndSubscribe(address(this), subscriptionOrRegistrantToCopy);
1354             } else {
1355                 if (subscriptionOrRegistrantToCopy != address(0)) {
1356                     operatorFilterRegistry.registerAndCopyEntries(address(this), subscriptionOrRegistrantToCopy);
1357                 } else {
1358                     operatorFilterRegistry.register(address(this));
1359                 }
1360             }
1361         }
1362     }
1363 
1364     modifier onlyAllowedOperator(address from) virtual {
1365         // Check registry code length to facilitate testing in environments without a deployed registry.
1366         if (address(operatorFilterRegistry).code.length > 0) {
1367             // Allow spending tokens from addresses with balance
1368             // Note that this still allows listings and marketplaces with escrow to transfer tokens if transferred
1369             // from an EOA.
1370             if (from == msg.sender) {
1371                 _;
1372                 return;
1373             }
1374             if (
1375                 !(
1376                     operatorFilterRegistry.isOperatorAllowed(address(this), msg.sender)
1377                         && operatorFilterRegistry.isOperatorAllowed(address(this), from)
1378                 )
1379             ) {
1380                 revert OperatorNotAllowed(msg.sender);
1381             }
1382         }
1383         _;
1384     }
1385 }
