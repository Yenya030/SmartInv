439     function transfer(address _to, uint256 _amount) public returns (bool success) {
440         if (balances[msg.sender] >= _amount && _amount > 0
441             && balances[_to] + _amount > balances[_to]) {
442             balances[msg.sender] -= _amount;
443             balances[_to] += _amount;
444             Transfer(msg.sender, _to, _amount);
445             return true;
446         } else {
447             return false;
448         }
449     }
450 
451     function transferFrom(address _from, address _to, uint256 _amount) public returns (bool success) {
452         if (balances[_from] >= _amount
453             && allowed[_from][msg.sender] >= _amount
454             && _amount > 0
455             && balances[_to] + _amount > balances[_to]) {
456             balances[_from] -= _amount;
457             allowed[_from][msg.sender] -= _amount;
458             balances[_to] += _amount;
459             Transfer(_from, _to, _amount);
460             return true;
461         } else {
462             return false;
463         }
464     }
465 
466     function approve(address _spender, uint256 _amount) public returns (bool success) {
467         allowed[msg.sender][_spender] = _amount;
468         Approval(msg.sender, _spender, _amount);
469         return true;
470     }
471 
472     function allowance(address _owner, address _spender) public constant returns (uint256 remaining) {
473         return allowed[_owner][_spender];
