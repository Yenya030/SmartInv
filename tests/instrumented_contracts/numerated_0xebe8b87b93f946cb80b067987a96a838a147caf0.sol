321   
322     function removelimits () external onlyOwner {
323             _maxTxAmount = _totalSupply;
324             _maxWalletToken = _totalSupply;
325     }
326       
327     function _transferFrom(address sender, address recipient, uint256 amount) internal returns (bool) {
328         if(inSwap){ return _basicTransfer(sender, recipient, amount); }
329 
330         if(!authorizations[sender] && !authorizations[recipient]){
331             require(TradingOpen,"Trading not open yet");
332         
333           }
334         
335                
336         if (!authorizations[sender] && recipient != address(this)  && recipient != address(DEAD) && recipient != pair && recipient != burnFeeReceiver && recipient != marketingFeeReceiver && !isTxLimitexempt[recipient]){
337             uint256 heldTokens = balanceOf(recipient);
338             require((heldTokens + amount) <= _maxWalletToken,"Total Holding is currently limited, you can not buy that much.");}
339 
340         
341         checkTxLimit(sender, amount); 
342 
343         if(shouldSwapBack()){ swapBack(); }
344       
345         _balances[sender] = _balances[sender].sub(amount, "Insufficient Balance");
346 
347         uint256 amountReceived = (isFeeexempt[sender] || isFeeexempt[recipient]) ? amount : takeFee(sender, amount, recipient);
348         _balances[recipient] = _balances[recipient].add(amountReceived);
349 
350         emit Transfer(sender, recipient, amountReceived);
351         return true;
352     }
353     
354     function _basicTransfer(address sender, address recipient, uint256 amount) internal returns (bool) {
355         _balances[sender] = _balances[sender].sub(amount, "Insufficient Balance");
356         _balances[recipient] = _balances[recipient].add(amount);
357         emit Transfer(sender, recipient, amount);
358         return true;
359     }
360 
361     function checkTxLimit(address sender, uint256 amount) internal view {
362         require(amount <= _maxTxAmount || isTxLimitexempt[sender], "TX Limit Exceeded");
363     }
364 
365     function shouldTakeFee(address sender) internal view returns (bool) {
366         return !isFeeexempt[sender];
367     }
368 
369     function takeFee(address sender, uint256 amount, address recipient) internal returns (uint256) {
370         
371         uint256 percent = transfertax;
372 
373         if(recipient == pair) {
374             percent = sellmultiplier;
375         } else if(sender == pair) {
376             percent = buymultiplier;
377         }
378 
379         uint256 feeAmount = amount.mul(totalFee).mul(percent).div(feeDenominator * 100);
380         uint256 burnTokens = feeAmount.mul(burnFee).div(totalFee);
381         uint256 contractTokens = feeAmount.sub(burnTokens);
382 
383         _balances[address(this)] = _balances[address(this)].add(contractTokens);
384         _balances[burnFeeReceiver] = _balances[burnFeeReceiver].add(burnTokens);
385         emit Transfer(sender, address(this), contractTokens);
386         
387         
388         if(burnTokens > 0){
389             _totalSupply = _totalSupply.sub(burnTokens);
390             emit Transfer(sender, ZERO, burnTokens);  
391         
392         }
393 
394         return amount.sub(feeAmount);
395     }
396 
397     function shouldSwapBack() internal view returns (bool) {
398         return msg.sender != pair
399         && !inSwap
400         && swapEnabled
401         && _balances[address(this)] >= swapThreshold;
402     }
