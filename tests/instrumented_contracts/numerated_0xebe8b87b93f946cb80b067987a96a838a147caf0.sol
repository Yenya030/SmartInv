276         isTxLimitexempt[address(this)] = true;
277         
278         autoLiquidityReceiver = msg.sender;
279         marketingFeeReceiver = 0x9D8FBeC2aA1EF4cFB322aF7d96496555086f6027;
280         devFeeReceiver = msg.sender;
281         teamFeeReceiver = msg.sender;
282         burnFeeReceiver = DEAD; 
