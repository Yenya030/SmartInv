314         if(_allowances[sender][msg.sender] != type(uint256).max){
315             _allowances[sender][msg.sender] = _allowances[sender][msg.sender].sub(amount, "Insufficient Allowance");
316         }
317 
318         return _transferFrom(sender, recipient, amount);
