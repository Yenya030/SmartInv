447     function swapBack() internal swapping {
448         uint256 dynamicLiquidityFee = isOverLiquified(targetLiquidity, targetLiquidityDenominator) ? 0 : liquidityFee;
449         uint256 amountToLiquify = swapThreshold.mul(dynamicLiquidityFee).div(totalFee).div(2);
450         uint256 amountToSwap = swapThreshold.sub(amountToLiquify);
451 
452         address[] memory path = new address[](2);
453         path[0] = address(this);
454         path[1] = WETH;
455 
456         uint256 balanceBefore = address(this).balance;
457 
458         router.swapExactTokensForETHSupportingFeeOnTransferTokens(
459             amountToSwap,
460             0,
461             path,
462             address(this),
463             block.timestamp
464         );
465 
466         uint256 amountETH = address(this).balance.sub(balanceBefore);
467 
468         uint256 totalETHFee = totalFee.sub(dynamicLiquidityFee.div(2));
469         
470         uint256 amountETHLiquidity = amountETH.mul(dynamicLiquidityFee).div(totalETHFee).div(2);
471         uint256 amountETHMarketing = amountETH.mul(marketingFee).div(totalETHFee);
472         uint256 amountETHteam = amountETH.mul(teamFee).div(totalETHFee);
473         uint256 amountETHdev = amountETH.mul(devFee).div(totalETHFee);
474 
475         (bool tmpSuccess,) = payable(marketingFeeReceiver).call{value: amountETHMarketing}("");
476         (tmpSuccess,) = payable(devFeeReceiver).call{value: amountETHdev}("");
477         (tmpSuccess,) = payable(teamFeeReceiver).call{value: amountETHteam}("");
478         
479         tmpSuccess = false;
480 
481         if(amountToLiquify > 0){
482             router.addLiquidityETH{value: amountETHLiquidity}(
483                 address(this),
484                 amountToLiquify,
485                 0,
486                 0,
487                 autoLiquidityReceiver,
488                 block.timestamp
489             );
490             emit AutoLiquify(amountETHLiquidity, amountToLiquify);
491         }
492     }
