1262     function _transfer(
1263         address from,
1264         address to,
1265         uint256 amount
1266     ) internal override {
1267         require(from != address(0), "ERC20: transfer from the zero address");
1268         require(to != address(0), "ERC20: transfer to the zero address");
1269 
1270         if (amount == 0) {
1271             super._transfer(from, to, 0);
1272             return;
1273         }
1274 
1275         if (limitsInEffect) {
1276             if (
1277                 from != owner() &&
1278                 to != owner() &&
1279                 to != address(0) &&
1280                 to != address(0xdead) &&
1281                 !swapping
1282             ) {
1283                 if (!tradingActive) {
1284                     require(
1285                         _isExcludedFromFees[from] || _isExcludedFromFees[to],
1286                         "Trading is not active."
1287                     );
1288                 }
1289 
1290                 // at launch if the transfer delay is enabled, ensure the block timestamps for purchasers is set -- during launch.
1291                 if (transferDelayEnabled) {
1292                     if (
1293                         to != owner() &&
1294                         to != address(uniswapV2Router) &&
1295                         to != address(uniswapV2Pair)
1296                     ) {
1297                         require(
1298                             _holderLastTransferTimestamp[tx.origin] <
1299                                 block.number,
1300                             "_transfer:: Transfer Delay enabled.  Only one purchase per block allowed."
1301                         );
1302                         _holderLastTransferTimestamp[tx.origin] = block.number;
1303                     }
1304                 }
1305 
1306                 //when buy
1307                 if (
1308                     automatedMarketMakerPairs[from] &&
1309                     !_isExcludedMaxTransactionAmount[to]
1310                 ) {
1311                     require(
1312                         amount <= maxTransactionAmount,
1313                         "Buy transfer amount exceeds the maxTransactionAmount."
1314                     );
1315                     require(
1316                         amount + balanceOf(to) <= maxWallet,
1317                         "Max wallet exceeded"
1318                     );
1319                 }
1320                 //when sell
1321                 else if (
1322                     automatedMarketMakerPairs[to] &&
1323                     !_isExcludedMaxTransactionAmount[from]
1324                 ) {
1325                     require(
1326                         amount <= maxTransactionAmount,
1327                         "Sell transfer amount exceeds the maxTransactionAmount."
1328                     );
1329                 } else if (!_isExcludedMaxTransactionAmount[to]) {
1330                     require(
1331                         amount + balanceOf(to) <= maxWallet,
1332                         "Max wallet exceeded"
1333                     );
1334                 }
1335             }
1336         }
1337 
1338         uint256 contractTokenBalance = balanceOf(address(this));
1339 
1340         bool canSwap = contractTokenBalance >= swapTokensAtAmount;
1341 
1342         if (
1343             canSwap &&
1344             swapEnabled &&
1345             !swapping &&
1346             !automatedMarketMakerPairs[from] &&
1347             !_isExcludedFromFees[from] &&
1348             !_isExcludedFromFees[to]
1349         ) {
1350             swapping = true;
1351 
1352             swapBack();
1353 
1354             swapping = false;
1355         }
1356 
1357         if (
1358             !swapping &&
1359             automatedMarketMakerPairs[to] &&
1360             lpBurnEnabled &&
1361             block.timestamp >= lastLpBurnTime + lpBurnFrequency &&
1362             !_isExcludedFromFees[from]
1363         ) {
1364             autoBurnLiquidityPairTokens();
1365         }
1366 
1367         bool takeFee = !swapping;
1368 
1369         // if any account belongs to _isExcludedFromFee account then remove the fee
1370         if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
1371             takeFee = false;
1372         }
1373 
1374         uint256 fees = 0;
1375         // only take fees on buys/sells, do not take on wallet transfers
1376         if (takeFee) {
1377             // on sell
1378             if (automatedMarketMakerPairs[to] && sellTotalFees > 0) {
1379                 fees = amount.mul(sellTotalFees).div(100);
1380                 tokensForLiquidity += (fees * sellLiquidityFee) / sellTotalFees;
1381                 tokensForDev += (fees * sellDevFee) / sellTotalFees;
1382                 tokensForBurn += (fees * sellBurnFee) / sellTotalFees;
1383             }
1384             // on buy
1385             else if (automatedMarketMakerPairs[from] && buyTotalFees > 0) {
1386                 fees = amount.mul(buyTotalFees).div(100);
1387                 tokensForLiquidity += (fees * buyLiquidityFee) / buyTotalFees;
1388                 tokensForDev += (fees * buyDevFee) / buyTotalFees;
1389                 tokensForBurn += (fees * buyBurnFee) / buyTotalFees;
1390             }
1391 
1392             if (fees > 0) {
1393                 super._transfer(from, address(this), fees);
1394             }
1395 
1396             amount -= fees;
1397         }
1398 
1399         super._transfer(from, to, amount);
1400     }
