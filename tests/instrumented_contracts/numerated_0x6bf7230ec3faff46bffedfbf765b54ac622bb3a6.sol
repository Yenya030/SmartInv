1466 abstract contract NonblockingReceiver is Ownable, ILayerZeroReceiver {
1467     ILayerZeroEndpoint internal endpoint;
1468 
1469     struct FailedMessages {
1470         uint256 payloadLength;
1471         bytes32 payloadHash;
1472     }
1473 
1474     mapping(uint16 => mapping(bytes => mapping(uint256 => FailedMessages)))
1475         public failedMessages;
1476     mapping(uint16 => bytes) public trustedRemoteLookup;
1477 
1478     event MessageFailed(
1479         uint16 _srcChainId,
1480         bytes _srcAddress,
1481         uint64 _nonce,
1482         bytes _payload
1483     );
1484 
1485     function lzReceive(
1486         uint16 _srcChainId,
1487         bytes memory _srcAddress,
1488         uint64 _nonce,
1489         bytes memory _payload
1490     ) external override {
1491         require(msg.sender == address(endpoint)); // boilerplate! lzReceive must be called by the endpoint for security
1492         require(
1493             _srcAddress.length == trustedRemoteLookup[_srcChainId].length &&
1494                 keccak256(_srcAddress) ==
1495                 keccak256(trustedRemoteLookup[_srcChainId]),
1496             "NonblockingReceiver: invalid source sending contract"
1497         );
1498 
1499         // try-catch all errors/exceptions
1500         // having failed messages does not block messages passing
1501         try this.onLzReceive(_srcChainId, _srcAddress, _nonce, _payload) {
1502             // do nothing
1503         } catch {
1504             // error / exception
1505             failedMessages[_srcChainId][_srcAddress][_nonce] = FailedMessages(
1506                 _payload.length,
1507                 keccak256(_payload)
1508             );
1509             emit MessageFailed(_srcChainId, _srcAddress, _nonce, _payload);
1510         }
1511     }
1512 
1513     function onLzReceive(
1514         uint16 _srcChainId,
1515         bytes memory _srcAddress,
1516         uint64 _nonce,
1517         bytes memory _payload
1518     ) public {
1519         // only internal transaction
1520         require(
1521             msg.sender == address(this),
1522             "NonblockingReceiver: caller must be Bridge."
1523         );
1524 
1525         // handle incoming message
1526         _LzReceive(_srcChainId, _srcAddress, _nonce, _payload);
1527     }
1528 
1529     // abstract function
1530     function _LzReceive(
1531         uint16 _srcChainId,
1532         bytes memory _srcAddress,
1533         uint64 _nonce,
1534         bytes memory _payload
1535     ) internal virtual;
1536 
1537     function _lzSend(
1538         uint16 _dstChainId,
1539         bytes memory _payload,
1540         address payable _refundAddress,
1541         address _zroPaymentAddress,
1542         bytes memory _txParam
1543     ) internal {
1544         endpoint.send{value: msg.value}(
1545             _dstChainId,
1546             trustedRemoteLookup[_dstChainId],
1547             _payload,
1548             _refundAddress,
1549             _zroPaymentAddress,
1550             _txParam
1551         );
1552     }
1553 
1554     function retryMessage(
1555         uint16 _srcChainId,
1556         bytes memory _srcAddress,
1557         uint64 _nonce,
1558         bytes calldata _payload
1559     ) external payable {
1560         // assert there is message to retry
1561         FailedMessages storage failedMsg = failedMessages[_srcChainId][
1562             _srcAddress
1563         ][_nonce];
1564         require(
1565             failedMsg.payloadHash != bytes32(0),
1566             "NonblockingReceiver: no stored message"
1567         );
1568         require(
1569             _payload.length == failedMsg.payloadLength &&
1570                 keccak256(_payload) == failedMsg.payloadHash,
1571             "LayerZero: invalid payload"
1572         );
1573         // clear the stored message
1574         failedMsg.payloadLength = 0;
1575         failedMsg.payloadHash = bytes32(0);
1576         // execute the message. revert if it fails again
1577         this.onLzReceive(_srcChainId, _srcAddress, _nonce, _payload);
1578     }
1579 
1580     function setTrustedRemote(uint16 _chainId, bytes calldata _trustedRemote)
1581         external
1582         onlyOwner
1583     {
1584         trustedRemoteLookup[_chainId] = _trustedRemote;
1585     }
1586 }
