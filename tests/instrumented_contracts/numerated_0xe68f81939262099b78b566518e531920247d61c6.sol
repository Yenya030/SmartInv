1357     IOperatorFilterRegistry constant operatorFilterRegistry =
1358         IOperatorFilterRegistry(0x000000000000AAeB6D7670E522A718067333cd4E);
1359 
1360     constructor(address subscriptionOrRegistrantToCopy, bool subscribe) {
1361         // If an inheriting token contract is deployed to a network without the registry deployed, the modifier
1362         // will not revert, but the contract will need to be registered with the registry once it is deployed in
1363         // order for the modifier to filter addresses.
1364         if (address(operatorFilterRegistry).code.length > 0) {
1365             if (subscribe) {
1366                 operatorFilterRegistry.registerAndSubscribe(address(this), subscriptionOrRegistrantToCopy);
1367             } else {
1368                 if (subscriptionOrRegistrantToCopy != address(0)) {
1369                     operatorFilterRegistry.registerAndCopyEntries(address(this), subscriptionOrRegistrantToCopy);
1370                 } else {
1371                     operatorFilterRegistry.register(address(this));
1372                 }
1373             }
1374         }
1375     }
1376 
1377     modifier onlyAllowedOperator(address from) virtual {
1378         // Check registry code length to facilitate testing in environments without a deployed registry.
1379         if (address(operatorFilterRegistry).code.length > 0) {
1380             // Allow spending tokens from addresses with balance
1381             // Note that this still allows listings and marketplaces with escrow to transfer tokens if transferred
1382             // from an EOA.
1383             if (from == msg.sender) {
1384                 _;
1385                 return;
1386             }
1387             if (
1388                 !(
1389                     operatorFilterRegistry.isOperatorAllowed(address(this), msg.sender)
1390                         && operatorFilterRegistry.isOperatorAllowed(address(this), from)
1391                 )
1392             ) {
1393                 revert OperatorNotAllowed(msg.sender);
1394             }
1395         }
1396         _;
1397     }
