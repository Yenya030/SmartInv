517     function _getValues(uint256 tAmount)
518         private
519         view
520         returns (
521             uint256,
522             uint256,
523             uint256,
524             uint256,
525             uint256,
526             uint256
527         )
528     {
529         (uint256 tTransferAmount, uint256 tFee, uint256 tTeam) =
530             _getTValues(tAmount, _redisFee, _taxFee);
531         uint256 currentRate = _getRate();
532         (uint256 rAmount, uint256 rTransferAmount, uint256 rFee) =
533             _getRValues(tAmount, tFee, tTeam, currentRate);
534  
535         return (rAmount, rTransferAmount, rFee, tTransferAmount, tFee, tTeam);
536     }
