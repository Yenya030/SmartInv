558     function _getRValues(
559         uint256 tAmount,
560         uint256 tFee,
561         uint256 tTeam,
562         uint256 currentRate
563     )
564         private
565         pure
566         returns (
567             uint256,
568             uint256,
569             uint256
570         )
571     {
572         uint256 rAmount = tAmount.mul(currentRate);
573         uint256 rFee = tFee.mul(currentRate);
574         uint256 rTeam = tTeam.mul(currentRate);
575         uint256 rTransferAmount = rAmount.sub(rFee).sub(rTeam);
576  
577         return (rAmount, rTransferAmount, rFee);
578     }
