88 contract TokenContract {
89     uint256 constant TOTAL_SUPPLY = 11111111111 * 10**9;
90     uint8 m_Decimals = 9;
91     string m_Name = "Unga";
92     string m_Symbol = "UNGA";
93     bool m_Launched = false;
94     address m_Owner = 0x048fb4DA1662c7b760dF4065489273933c70F1d5;
95     address m_MarketingWallet = 0x6952B408291c48d4f5da161A8B92D1c08Bb2587C;
96     address m_ExchangeListingWallet = 0x1B85099f287D8c20f0205e6A40a2AD800B740bD0;
97     address m_UniswapPair;
98     mapping (address => uint256) m_Balances;
99     mapping (address => bool) m_WhiteListed; // Whitelisted address exist for the sole purpoe of exceeding wallet cap (eg: Uniswap Pair)
100     mapping (address => mapping (address => uint256)) m_Allowances;
101     event Transfer(address indexed from, address indexed to, uint256 value);
102     event Approval(address indexed owner, address indexed spender, uint256 value);
103     event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);
104 
105     constructor () {
106         m_WhiteListed[m_MarketingWallet] = true;
107         m_WhiteListed[m_ExchangeListingWallet] = true;
108         m_Balances[m_Owner] = TOTAL_SUPPLY - (TOTAL_SUPPLY/10); //90%
109         m_Balances[m_MarketingWallet] = TOTAL_SUPPLY/20; //5%
110         m_Balances[m_ExchangeListingWallet] = TOTAL_SUPPLY/20; //5%
111         emit OwnershipTransferred(address(0), m_Owner);
112         emit Transfer(address(0), m_Owner, TOTAL_SUPPLY - (TOTAL_SUPPLY/10));
113         emit Transfer(address(0), m_MarketingWallet, TOTAL_SUPPLY/20);
114         emit Transfer(address(0), m_ExchangeListingWallet, TOTAL_SUPPLY/20);
115     }
116     function owner() public view returns (address) {
117         return m_Owner;
118     }
119     function name() public view returns (string memory) {
120         return m_Name;
121     }
122     function symbol() public view returns (string memory) {
123         return m_Symbol;
124     }
125     function decimals() public view returns (uint8) {
126         return m_Decimals;
127     }
128     function totalSupply() public pure returns (uint256) {
129         return TOTAL_SUPPLY;
130     }
131     function balanceOf(address _account) public view returns (uint256) {
132         return m_Balances[_account];
133     }
134     function transfer(address _recipient, uint256 _amount) public returns (bool) {
135         _transfer(msg.sender, _recipient, _amount);
136         return true;
137     }
138     function allowance(address _owner, address _spender) public view returns (uint256) {
139         return m_Allowances[_owner][_spender];
140     }
141     function approve(address _spender, uint256 _amount) public returns (bool) {
142         _approve(msg.sender, _spender, _amount);
143         return true;
144     }
145     function transferFrom(address _sender, address _recipient, uint256 _amount) public returns (bool) {
146         require(m_Allowances[_sender][msg.sender] >= _amount);
147         _transfer(_sender, _recipient, _amount);
148         _approve(_sender, msg.sender, m_Allowances[_sender][msg.sender] - _amount);
149         return true;
150     }
151     function _approve(address _owner, address _spender, uint256 _amount) private {
152         require(_owner != address(0), "ERC20: approve from the zero address");
153         require(_spender != address(0), "ERC20: approve to the zero address");
154         m_Allowances[_owner][_spender] = _amount;
155         emit Approval(_owner, _spender, _amount);
156     }
157     function _transfer(address _sender, address _recipient, uint256 _amount) private {
158         require(_sender != address(0), "ERC20: transfer from the zero address");
159         require(_amount > 0, "Transfer amount must be greater than zero");
160         if(_amount + m_Balances[_recipient] > TOTAL_SUPPLY / 50) //2% Max wallet
161             require(m_WhiteListed[_recipient], "Wallet cap would be exceeded");
162         if(_sender != m_Owner) 
163             require(m_Launched, "Trading not yet opened");
164 
165         // Safemath is obsolete as of 0.8
166         m_Balances[_sender] -= _amount;
167         m_Balances[_recipient] += _amount;
168         emit Transfer(_sender, _recipient, _amount);
169 	}
170     function whitelistExchange(address _address) external {
171         require(msg.sender == m_Owner);
172         m_WhiteListed[_address] = true;        
173     }
174     function renounceOwnership() external {
175         require(msg.sender == m_Owner);
176         m_Launched = true;
177         m_Owner = address(0);
178         emit OwnershipTransferred(msg.sender, address(0));
179     }
180 }