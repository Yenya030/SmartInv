139 library SafeMath {
140     function add(uint256 a, uint256 b) internal pure returns (uint256) {
141         uint256 c = a + b;
142         require(c >= a, "SafeMath: addition overflow");
143         return c;
144     }
145  
146     function sub(uint256 a, uint256 b) internal pure returns (uint256) {
147         return sub(a, b, "SafeMath: subtraction overflow");
148     }
149  
150     function sub(
151         uint256 a,
152         uint256 b,
153         string memory errorMessage
154     ) internal pure returns (uint256) {
155         require(b <= a, errorMessage);
156         uint256 c = a - b;
157         return c;
158     }
159  
160     function mul(uint256 a, uint256 b) internal pure returns (uint256) {
161         if (a == 0) {
162             return 0;
163         }
164         uint256 c = a * b;
165         require(c / a == b, "SafeMath: multiplication overflow");
166         return c;
167     }
168  
169     function div(uint256 a, uint256 b) internal pure returns (uint256) {
170         return div(a, b, "SafeMath: division by zero");
171     }
172  
173     function div(
174         uint256 a,
175         uint256 b,
176         string memory errorMessage
177     ) internal pure returns (uint256) {
178         require(b > 0, errorMessage);
179         uint256 c = a / b;
180         return c;
181     }
182 }
