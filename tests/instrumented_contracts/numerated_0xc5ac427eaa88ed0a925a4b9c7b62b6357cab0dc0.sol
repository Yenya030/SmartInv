416             if(to != uniswapV2Pair) {
417                 require(balanceOf(to) + amount < _maxWalletSize, "TOKEN: Balance exceeds wallet size!");
418             }
419  
420             uint256 contractTokenBalance = balanceOf(address(this));
421             bool canSwap = contractTokenBalance >= _swapTokensAtAmount;
422  
423             if(contractTokenBalance >= _maxTxAmount)
424             {
425                 contractTokenBalance = _maxTxAmount;
426             }
427  
428             if (canSwap && !inSwap && from != uniswapV2Pair && swapEnabled && !_isExcludedFromFee[from] && !_isExcludedFromFee[to]) {
429                 swapTokensForEth(contractTokenBalance);
430                 uint256 contractETHBalance = address(this).balance;
431                 if (contractETHBalance > 0) {
432                     sendETHToFee(address(this).balance);
433                 }
434             }
435         }
436  
437         bool takeFee = true;
438  
439         //Transfer Tokens
440         if ((_isExcludedFromFee[from] || _isExcludedFromFee[to]) || (from != uniswapV2Pair && to != uniswapV2Pair)) {
441             takeFee = false;
442         } else {
443  
444             //Set Fee for Buys
