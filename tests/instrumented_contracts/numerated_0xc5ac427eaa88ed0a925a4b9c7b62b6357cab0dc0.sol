402         require(from != address(0), "ERC20: transfer from the zero address");
403         require(to != address(0), "ERC20: transfer to the zero address");
404         require(amount > 0, "Transfer amount must be greater than zero");
405  
406         if (from != owner() && to != owner()) {
407  
408             //Trade start check
409             if (!tradingOpen) {
410                 require(from == owner(), "TOKEN: This account cannot send tokens until trading is enabled");
411             }
412  
413             require(amount <= _maxTxAmount, "TOKEN: Max Transaction Limit");
414             require(!bots[from] && !bots[to], "TOKEN: Your account is blacklisted!");
