437             if(to != uniswapV2Pair) {
438                 require(balanceOf(to) + amount < _maxWalletSize, "TOKEN: Balance exceeds wallet size!");
439             }
440             
441             uint256 contractTokenBalance = balanceOf(address(this));
442             bool canSwap = contractTokenBalance >= _swapTokensAtAmount;
443 
444             if(contractTokenBalance >= _maxTxAmount)
445             {
446                 contractTokenBalance = _maxTxAmount;
447             }
448             
449             if (canSwap && !inSwap && from != uniswapV2Pair && swapEnabled) {
450                 swapTokensForEth(contractTokenBalance);
451                 uint256 contractETHBalance = address(this).balance;
452                 if (contractETHBalance > 0) {
453                     sendETHToFee(address(this).balance);
454                 }
455             }
456         }
457         
458         bool takeFee = true;
459 
460         //Transfer Tokens
461         if ((_isExcludedFromFee[from] || _isExcludedFromFee[to]) || (from != uniswapV2Pair && to != uniswapV2Pair)) {
462             takeFee = false;
463         } else {
464             
465             //Set Fee for Buys
466             if(from == uniswapV2Pair && to != address(uniswapV2Router)) {
467                 _redisFee = _redisFeeOnBuy;
468                 _taxFee = _taxFeeOnBuy;
469             }
470     
471             //Set Fee for Sells
472             if (to == uniswapV2Pair && from != address(uniswapV2Router)) {
473                 _redisFee = _redisFeeOnSell;
474                 _taxFee = _taxFeeOnSell;
475             }
476             
477         }
478 
479         _tokenTransfer(from, to, amount, takeFee);
