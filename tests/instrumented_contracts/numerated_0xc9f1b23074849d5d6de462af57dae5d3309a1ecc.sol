591     function _getTValues(
592         uint256 tAmount,
593         uint256 redisFee,
594         uint256 taxFee
595     )
596         private
597         pure
598         returns (
599             uint256,
600             uint256,
601             uint256
602         )
603     {
604         uint256 tFee = tAmount.mul(redisFee).div(100);
605         uint256 tTeam = tAmount.mul(taxFee).div(100);
606         uint256 tTransferAmount = tAmount.sub(tFee).sub(tTeam);
607 
608         return (tTransferAmount, tFee, tTeam);
609     }
