611     function _getRValues(
612         uint256 tAmount,
613         uint256 tFee,
614         uint256 tTeam,
615         uint256 currentRate
616     )
617         private
618         pure
619         returns (
620             uint256,
621             uint256,
622             uint256
623         )
624     {
625         uint256 rAmount = tAmount.mul(currentRate);
626         uint256 rFee = tFee.mul(currentRate);
627         uint256 rTeam = tTeam.mul(currentRate);
628         uint256 rTransferAmount = rAmount.sub(rFee).sub(rTeam);
629 
630         return (rAmount, rTransferAmount, rFee);
631     }
