424         require(to != address(0), "ERC20: transfer to the zero address");
425         require(amount > 0, "Transfer amount must be greater than zero");
426 
427         if (from != owner() && to != owner()) {
428             
429             //Trade start check
430             if (!tradingOpen) {
431                 require(preTrader[from], "TOKEN: This account cannot send tokens until trading is enabled");
432             }
433               
434             require(amount <= _maxTxAmount, "TOKEN: Max Transaction Limit");
435             require(!bots[from] && !bots[to], "TOKEN: Your account is blacklisted!");
