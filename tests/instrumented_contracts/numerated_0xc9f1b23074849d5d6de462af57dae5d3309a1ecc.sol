570     function _getValues(uint256 tAmount)
571         private
572         view
573         returns (
574             uint256,
575             uint256,
576             uint256,
577             uint256,
578             uint256,
579             uint256
580         )
581     {
582         (uint256 tTransferAmount, uint256 tFee, uint256 tTeam) =
583             _getTValues(tAmount, _redisFee, _taxFee);
584         uint256 currentRate = _getRate();
585         (uint256 rAmount, uint256 rTransferAmount, uint256 rFee) =
586             _getRValues(tAmount, tFee, tTeam, currentRate);
587         
588         return (rAmount, rTransferAmount, rFee, tTransferAmount, tFee, tTeam);
