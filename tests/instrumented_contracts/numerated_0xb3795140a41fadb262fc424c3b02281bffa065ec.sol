187 contract IMoneyTokenPromoInterface {
188     /* This is a slight change to the ERC20 base standard.
189     function totalSupply() constant returns (uint256 supply);
190     is replaced with:
191     uint256 public totalSupply;
192     This automatically creates a getter function for the totalSupply.
193     This is moved to the base contract since public getter functions are not
194     currently recognised as an implementation of the matching abstract
195     function by the compiler.
196     */
197     /// total amount of tokens
198     uint256 public totalSupply;
199 
200     /// @param _owner The address from which the balance will be retrieved
201     /// @return The balance
202     function balanceOf(address _owner) public view returns (uint256 balance);
203 
204     /// @notice send `_value` token to `_to` from `msg.sender`
205     /// @param _to The address of the recipient
206     /// @param _value The amount of token to be transferred
207     /// @return Whether the transfer was successful or not
208     function transfer(address _to, uint256 _value) public returns (bool success);
209 
210     /// @notice send `_value` token to `_to` from `_from` on the condition it is approved by `_from`
211     /// @param _from The address of the sender
212     /// @param _to The address of the recipient
213     /// @param _value The amount of token to be transferred
214     /// @return Whether the transfer was successful or not
215     function transferFrom(address _from, address _to, uint256 _value) public returns (bool success);
216 
217     /// @notice `msg.sender` approves `_spender` to spend `_value` tokens
218     /// @param _spender The address of the account able to transfer the tokens
219     /// @param _value The amount of tokens to be approved for transfer
220     /// @return Whether the approval was successful or not
221     function approve(address _spender, uint256 _value) public returns (bool success);
222 
223     /// @param _owner The address of the account owning tokens
224     /// @param _spender The address of the account able to transfer the tokens
225     /// @return Amount of remaining tokens allowed to spent
226     function allowance(address _owner, address _spender) public view returns (uint256 remaining);
227 
228     // solhint-disable-next-line no-simple-event-func-name  
229     event Transfer(address indexed _from, address indexed _to, uint256 _value); 
230     event Approval(address indexed _owner, address indexed _spender, uint256 _value);
231 }
232 contract MoneyTokenPromo is IMoneyTokenPromoInterface {
233 
234     uint256 constant private MAX_UINT256 = 2**256 - 1;
235     mapping (address => uint256) public balances;
236     mapping (address => mapping (address => uint256)) public allowed;
237     /*
238     NOTE:
239     The following variables are OPTIONAL vanities. One does not have to include them.
240     They allow one to customise the token contract & in no way influences the core functionality.
241     Some wallets/interfaces might not even bother to look at this information.
242     */
243     string public name;                   //fancy name: eg Simon Bucks
244     uint8 public decimals;                //How many decimals to show.
245     string public symbol;                 //An identifier: eg SBX
246 
247     function MoneyTokenPromo(
248         uint256 _initialAmount,
249         string _tokenName,
250         uint8 _decimalUnits,
251         string _tokenSymbol
252     ) public {
253         balances[msg.sender] = _initialAmount;               // Give the creator all initial tokens
254         totalSupply = _initialAmount;                        // Update total supply
255         name = _tokenName;                                   // Set the name for display purposes
256         decimals = _decimalUnits;                            // Amount of decimals for display purposes
257         symbol = _tokenSymbol;                               // Set the symbol for display purposes
258     }
259 
260     function transfer(address _to, uint256 _value) public returns (bool success) {
261         require(balances[msg.sender] >= _value);
262         balances[msg.sender] -= _value;
263         balances[_to] += _value;
264         Transfer(msg.sender, _to, _value);
265         return true;
266     }
267 
268     function transferFrom(address _from, address _to, uint256 _value) public returns (bool success) {
269         uint256 allowance = allowed[_from][msg.sender];
270         require(balances[_from] >= _value && allowance >= _value);
271         balances[_to] += _value;
272         balances[_from] -= _value;
273         if (allowance < MAX_UINT256) {
274             allowed[_from][msg.sender] -= _value;
275         }
276         Transfer(_from, _to, _value);
277         return true;
278     }
279 
280     function balanceOf(address _owner) public view returns (uint256 balance) {
281         return balances[_owner];
282     }
283 
284     function approve(address _spender, uint256 _value) public returns (bool success) {
285         allowed[msg.sender][_spender] = _value;
286         Approval(msg.sender, _spender, _value);
287         return true;
288     }
289 
290     function allowance(address _owner, address _spender) public view returns (uint256 remaining) {
291         return allowed[_owner][_spender];
292     }   
293 }